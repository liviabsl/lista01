#include <stdio.h>

int main(void)
{
    
   void questao19(void){
        float lado1, lado2, lado3;
        printf("Lado 1: ");
        scanf("%f", &lado1);
        printf("Lado 2: ");
        scanf("%f", &lado2);
        printf("Lado 3: ");
        scanf("%f", &lado3);
     
        
        if(lado1 == lado2 && lado2 == lado3){
                printf("Equilátero");
        }
        else if(lado1 == lado2 || lado2 == lado3 || lado1==lado3){
                printf("Isósceles");
        }
        else{
            printf("Escaleno");
        }
      
   }
      
      questao19();
    
}
