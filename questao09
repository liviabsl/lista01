#include <stdio.h>

int main(void)
{
    
   void questao09(void){
        float numero1;
        printf("Numero: ");
        scanf("%f", &numero1);
        
        if(numero1>100){
            printf("O número %f é maior que 100", numero1);
        }
        else{
            printf("O número %f não é maior que 100", numero1);
        }
        
   }
      
      questao09();
    
}
