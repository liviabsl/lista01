#include <stdio.h>

int main(void)
{
    
   void questao21(void){
        int numero1, numero2, numero3, maior,medio, menor;
        printf("Numero 1: ");
        scanf("%d", &numero1);
        printf("Numero 2: ");
        scanf("%d", &numero2);
        printf("Numero 3: ");
        scanf("%d", &numero3);
        
        
        if(numero1 > numero2 && numero1> numero3){
            maior = numero1;
            if(numero2>numero3){
                medio=numero2;
                menor=numero3;
            }
            else{
                medio=numero3;
                menor=numero2;
            }
                printf("%d\n %d\n %d\n", menor, medio, maior);
        }
        
        else if(numero2 > numero1 && numero2>numero3){
            maior=numero2;
            if(numero1>numero3){
                medio=numero1;
                menor=numero3;
            }
            else{
                medio=numero3;
                menor=numero1;
            }
            printf("%d\n %d\n %d\n", menor, medio, maior);
        }
        
        else if(numero1 == numero2 && numero2 == numero3){
            printf("Numeros identicos!");
        }
        
        else{
            maior=numero3;
            if(numero1>numero2){
                medio=numero1;
                menor=numero2;
            }
            else{
                medio=numero2;
                menor=numero1;
            }
            printf("%d\n %d\n %d\n", menor, medio, maior);
        }
        
       
       
        
   }
      
      questao21();
    
}
